{
  "name": "pf2e-token-art",
  "title": "PF2e Token Art",
  "description": "A module to add art to system tokens",
  "authors": [
    {
      "name": "Zullock",
      "discord": "Zullock#2883"
    }
  ],
  "version": "2.0.0",
  "compatibility": {
    "minimum": "10.291",
    "verified": "12"
  },
  "url": "https://github.com/Zullock/pf2e-token-art",
  "manifest": "https://raw.githubusercontent.com/Zullock/pf2e-token-art/main/module.json",
  "download": "https://github.com/Zullock/pf2e-token-art/releases/download/v2.0.0/module.zip",
  "changelog": "https://github.com/Zullock/pf2e-token-art/blob/main/CHANGELOG.md",
  "flags": {
      "compendiumArtMappings": {
        "pf2e": {
          "mapping": "modules/pf2e-token-art/map-art-tokens.json"
        }
      },
      "tokenRingSubjectMappings": {
        "modules/pf2e-token-art/tokens/": "modules/pf2e-token-art/subjects/"
      }
  },
  "relationships": {
    "systems": [
      {
        "id": "pf2e",
        "type": "system",
        "manifest": "https://raw.githubusercontent.com/foundryvtt/pf2e/master/system.json",
        "compatibility": {
          "minimum": "4.8.1"
        }
      }
    ]
  },
  "recommends": [
    {
      "id": "pf2e-tokens-characters",
      "type": "module",
      "compatibility": {}
    }
  ]
}